{
    "manifest_version": 3,
    "name": "Virtual Keyboard Test",
    "version": "1.0",
    "description": "Test virtual keyboard extension",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["keyboard.js"],
        "css": ["keyboard.css"],
        "run_at": "document_end"
      }
    ]
  }